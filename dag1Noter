let num = []
// Laver array med 1000 elementer
for(let i = 0; i < 2000; i++){
    num.push(i)
}

console.log(num[1000])
// Køre igennem alle elementer og printer dem ud
for(let y = 0; y < num.length; y++){
    console.log(num[y])
}

for(let x = 0; x < num.length; x++){
    num[x] = num[x] * 2 
    console.log(num[x])
}

