//Boolean operators
// ----------- FØRSTE DEL AF DAGEN
//Laver variable
let a = true
let b = true
let c = 4
let d = 10

//Eksempel på en if/else statement
if(b){
    console.log('Du har ret')
}else if(c > 100){
    console.log('hej2')
}
else{
    console.log('noone of the above')
}

//! modsat af sandt

//Betyder Og
// &&
//Betyder eller
// ||

//DET SAMME:
// ==
//IKKE DET SAMME
// !=

//Lav en if/else statement
//Brug ovenstående variabler, a,b,c,d
//Opfyld disse kriterier:
//Er a falsk? Hvis ja - print 'Hej'
//er c+d mindre end 10 ? - Hvis ja print 'hej2'
//Ellers print 'INGENTING'

//Hvis A ikke er sand
if(!a){
    console.log('Hej')
}
//Ellers hvis c+d er mindre end 10
else if(c+d < 10){
    console.log('hej2')
}
//Ellers gør dette
else{
    console. log('ingenting')
}


